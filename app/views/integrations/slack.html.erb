<% content_for :title, "Slack Integration - ActiveRabbit" %>

<div class="min-h-screen bg-[#0a0a0f]">
  <!-- Header -->
  <header class="py-4 px-4 sm:px-6 lg:px-8 border-b border-white/5">
    <div class="max-w-6xl mx-auto flex items-center justify-between">
      <a href="https://www.activerabbit.ai/" class="flex items-center space-x-3">
        <%= image_tag "faviicons/favicon-32.png", class: "rounded-full", alt: "ActiveRabbit" %>
        <span class="text-xl font-bold text-white">ActiveRabbit</span>
      </a>
      <div class="flex items-center space-x-3">
        <% if user_signed_in? %>
          <%= link_to "Dashboard", dashboard_path, class: "text-gray-400 hover:text-white transition text-sm" %>
        <% else %>
          <%= link_to "Login", new_user_session_path, class: "text-gray-400 hover:text-white transition text-sm" %>
          <%= link_to "Start Free Trial", new_user_registration_path, class: "px-4 py-2 bg-gradient-to-r from-violet-600 to-indigo-600 hover:from-violet-500 hover:to-indigo-500 text-white text-sm font-medium rounded-lg transition" %>
        <% end %>
      </div>
    </div>
  </header>

  <!-- Hero Section -->
  <section class="py-20 px-4 sm:px-6 lg:px-8 relative overflow-hidden">
    <!-- Background gradient -->
    <div class="absolute inset-0 bg-gradient-to-b from-violet-600/10 via-transparent to-transparent"></div>
    <div class="absolute top-0 left-1/2 -translate-x-1/2 w-[800px] h-[400px] bg-violet-600/20 blur-[120px] rounded-full"></div>
    
    <div class="max-w-4xl mx-auto text-center relative z-10">
      <div class="inline-flex items-center justify-center w-20 h-20 bg-white/5 backdrop-blur border border-white/10 rounded-2xl mb-4">
        <svg xmlns="http://www.w3.org/2000/svg" width="46" height="46" viewBox="0 0 16 16">
          <path fill="#ECB22E" d="M5.89 3.362c-.926 0-1.682-.756-1.682-1.681S4.964 0 5.89 0s1.68.756 1.68 1.68v1.682zm0 .846c.924 0 1.68.756 1.68 1.681S6.814 7.57 5.89 7.57H1.68C.757 7.57 0 6.814 0 5.89c0-.926.756-1.682 1.68-1.682z"/>
          <path fill="#36C5F0" d="M12.639 5.89c0-.926.755-1.682 1.68-1.682S16 4.964 16 5.889s-.756 1.681-1.68 1.681h-1.681zm-.848 0c0 .924-.755 1.68-1.68 1.68A1.685 1.685 0 0 1 8.43 5.89V1.68C8.43.757 9.186 0 10.11 0c.926 0 1.681.756 1.681 1.68z"/>
          <path fill="#2EB67D" d="M10.11 12.639c.926 0 1.682.756 1.682 1.681S11.036 16 10.11 16s-1.681-.756-1.681-1.68v-1.682zm0-.847c-.924 0-1.68-.755-1.68-1.68s.756-1.681 1.68-1.681h4.21c.924 0 1.68.756 1.68 1.68 0 .926-.756 1.681-1.68 1.681z"/>
          <path fill="#E01E5A" d="M3.362 10.11c0 .926-.756 1.681-1.681 1.681S0 11.036 0 10.111.756 8.43 1.68 8.43h1.682zm.846 0c0-.924.756-1.68 1.681-1.68s1.681.756 1.681 1.68v4.21c0 .924-.756 1.68-1.68 1.68a1.685 1.685 0 0 1-1.682-1.68z"/>
        </svg>
      </div></br>
      
      <div class="inline-flex items-center px-3 py-1 rounded-full bg-violet-500/10 border border-violet-500/20 mb-6">
        <span class="text-violet-400 text-sm font-medium">INTEGRATION</span>
      </div>
      
      <h1 class="text-4xl sm:text-5xl lg:text-6xl font-bold text-white mb-6 leading-tight">
        Slack Integration
      </h1>
      <p class="text-xl text-gray-400 max-w-2xl mx-auto mb-10 leading-relaxed">
        Get instant error alerts in your Slack workspace. Never miss a critical bug again.
      </p>

      <!-- CTA Buttons -->
      <div class="flex flex-col sm:flex-row items-center justify-center gap-4">
        <% if user_signed_in? %>
          <%= link_to dashboard_path, class: "inline-flex items-center px-6 py-3 bg-gradient-to-r from-violet-600 to-indigo-600 hover:from-violet-500 hover:to-indigo-500 text-white font-medium rounded-lg transition shadow-lg shadow-violet-500/25" do %>
            <svg class="w-5 h-5 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10.325 4.317c.426-1.756 2.924-1.756 3.35 0a1.724 1.724 0 002.573 1.066c1.543-.94 3.31.826 2.37 2.37a1.724 1.724 0 001.065 2.572c1.756.426 1.756 2.924 0 3.35a1.724 1.724 0 00-1.066 2.573c.94 1.543-.826 3.31-2.37 2.37a1.724 1.724 0 00-2.572 1.065c-.426 1.756-2.924 1.756-3.35 0a1.724 1.724 0 00-2.573-1.066c-1.543.94-3.31-.826-2.37-2.37a1.724 1.724 0 00-1.065-2.572c-1.756-.426-1.756-2.924 0-3.35a1.724 1.724 0 001.066-2.573c-.94-1.543.826-3.31 2.37-2.37.996.608 2.296.07 2.572-1.065z"></path>
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z"></path>
            </svg>
            Go to Project Settings
          <% end %>
        <% else %>
          <%= link_to new_user_registration_path, class: "inline-flex items-center px-6 py-3 bg-gradient-to-r from-violet-600 to-indigo-600 hover:from-violet-500 hover:to-indigo-500 text-white font-medium rounded-lg transition shadow-lg shadow-violet-500/25" do %>
            Get Started Free
          <% end %>
          <a href="https://github.com/activerabbit/activerabbit" class="inline-flex items-center px-6 py-3 bg-white/5 hover:bg-white/10 border border-white/10 text-white font-medium rounded-lg transition">
            <svg class="w-5 h-5 mr-2" fill="currentColor" viewBox="0 0 24 24">
              <path d="M12 0C5.37 0 0 5.37 0 12c0 5.31 3.435 9.795 8.205 11.385.6.105.825-.255.825-.57 0-.285-.015-1.23-.015-2.235-3.015.555-3.795-.735-4.035-1.41-.135-.345-.72-1.41-1.23-1.695-.42-.225-1.02-.78-.015-.795.945-.015 1.62.87 1.845 1.23 1.08 1.815 2.805 1.305 3.495.99.105-.78.42-1.305.765-1.605-2.67-.3-5.46-1.335-5.46-5.925 0-1.305.465-2.385 1.23-3.225-.12-.3-.54-1.53.12-3.18 0 0 1.005-.315 3.3 1.23.96-.27 1.98-.405 3-.405s2.04.135 3 .405c2.295-1.56 3.3-1.23 3.3-1.23.66 1.65.24 2.88.12 3.18.765.84 1.23 1.905 1.23 3.225 0 4.605-2.805 5.625-5.475 5.925.435.375.81 1.095.81 2.22 0 1.605-.015 2.895-.015 3.3 0 .315.225.69.825.57A12.02 12.02 0 0024 12c0-6.63-5.37-12-12-12z"/>
            </svg>
            View on GitHub
          </a>
        <% end %>
      </div>
    </div>
  </section>

  <!-- How It Works -->
  <section class="py-20 px-4 sm:px-6 lg:px-8">
    <div class="max-w-6xl mx-auto">
      <div class="text-center mb-16">
        <h2 class="text-3xl sm:text-4xl font-bold text-white mb-4">How It Works</h2>
        <p class="text-gray-400 max-w-2xl mx-auto">Connect your Slack workspace in minutes and start receiving real-time error alerts.</p>
      </div>
      
      <div class="grid md:grid-cols-3 gap-8">
        <!-- Step 1 -->
        <div class="relative group">
          <div class="absolute inset-0 bg-gradient-to-r from-violet-600/20 to-indigo-600/20 rounded-2xl blur-xl opacity-0 group-hover:opacity-100 transition-opacity"></div>
          <div class="relative bg-white/5 border border-white/10 rounded-2xl p-8 hover:border-violet-500/50 transition">
            <div class="w-12 h-12 bg-gradient-to-r from-violet-600 to-indigo-600 rounded-xl flex items-center justify-center text-white font-bold text-lg mb-6">1</div>
            <h3 class="text-xl font-semibold text-white mb-3">Connect Your Workspace</h3>
            <p class="text-gray-400 leading-relaxed">
              Click "Add to Slack" in your project settings to authorize ActiveRabbit with your Slack workspace.
            </p>
          </div>
        </div>
        
        <!-- Step 2 -->
        <div class="relative group">
          <div class="absolute inset-0 bg-gradient-to-r from-violet-600/20 to-indigo-600/20 rounded-2xl blur-xl opacity-0 group-hover:opacity-100 transition-opacity"></div>
          <div class="relative bg-white/5 border border-white/10 rounded-2xl p-8 hover:border-violet-500/50 transition">
            <div class="w-12 h-12 bg-gradient-to-r from-violet-600 to-indigo-600 rounded-xl flex items-center justify-center text-white font-bold text-lg mb-6">2</div>
            <h3 class="text-xl font-semibold text-white mb-3">Automatic Channel Setup</h3>
            <p class="text-gray-400 leading-relaxed">
              We automatically create an <code class="bg-white/10 px-1.5 py-0.5 rounded text-violet-300">#active_rabbit_alert</code> channel for your notifications.
            </p>
          </div>
        </div>
        
        <!-- Step 3 -->
        <div class="relative group">
          <div class="absolute inset-0 bg-gradient-to-r from-violet-600/20 to-indigo-600/20 rounded-2xl blur-xl opacity-0 group-hover:opacity-100 transition-opacity"></div>
          <div class="relative bg-white/5 border border-white/10 rounded-2xl p-8 hover:border-violet-500/50 transition">
            <div class="w-12 h-12 bg-gradient-to-r from-violet-600 to-indigo-600 rounded-xl flex items-center justify-center text-white font-bold text-lg mb-6">3</div>
            <h3 class="text-xl font-semibold text-white mb-3">Get Real-time Alerts</h3>
            <p class="text-gray-400 leading-relaxed">
              Receive instant notifications when errors occur, with full stack traces and AI-powered fix suggestions.
            </p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Features -->
  <section class="py-20 px-4 sm:px-6 lg:px-8 bg-gradient-to-b from-transparent via-violet-600/5 to-transparent">
    <div class="max-w-6xl mx-auto">
      <div class="text-center mb-16">
        <h2 class="text-3xl sm:text-4xl font-bold text-white mb-4">What You Get</h2>
        <p class="text-gray-400 max-w-2xl mx-auto">Everything you need to stay on top of production errors.</p>
      </div>
      
      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
        <div class="bg-white/5 border border-white/10 rounded-xl p-6 hover:border-violet-500/30 transition">
          <div class="w-10 h-10 bg-emerald-500/20 rounded-lg flex items-center justify-center mb-4">
            <svg class="w-5 h-5 text-emerald-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"></path>
            </svg>
          </div>
          <h3 class="text-lg font-semibold text-white mb-2">Instant Notifications</h3>
          <p class="text-gray-400 text-sm">Get alerted within seconds when new errors occur in production.</p>
        </div>
        
        <div class="bg-white/5 border border-white/10 rounded-xl p-6 hover:border-violet-500/30 transition">
          <div class="w-10 h-10 bg-blue-500/20 rounded-lg flex items-center justify-center mb-4">
            <svg class="w-5 h-5 text-blue-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"></path>
            </svg>
          </div>
          <h3 class="text-lg font-semibold text-white mb-2">Rich Error Context</h3>
          <p class="text-gray-400 text-sm">See error messages, stack traces, and affected users right in Slack.</p>
        </div>
        
        <div class="bg-white/5 border border-white/10 rounded-xl p-6 hover:border-violet-500/30 transition">
          <div class="w-10 h-10 bg-violet-500/20 rounded-lg flex items-center justify-center mb-4">
            <svg class="w-5 h-5 text-violet-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.663 17h4.673M12 3v1m6.364 1.636l-.707.707M21 12h-1M4 12H3m3.343-5.657l-.707-.707m2.828 9.9a5 5 0 117.072 0l-.548.547A3.374 3.374 0 0014 18.469V19a2 2 0 11-4 0v-.531c0-.895-.356-1.754-.988-2.386l-.548-.547z"></path>
            </svg>
          </div>
          <h3 class="text-lg font-semibold text-white mb-2">AI-Powered Insights</h3>
          <p class="text-gray-400 text-sm">Get AI-generated summaries and suggested fixes for each error.</p>
        </div>
        
        <div class="bg-white/5 border border-white/10 rounded-xl p-6 hover:border-violet-500/30 transition">
          <div class="w-10 h-10 bg-amber-500/20 rounded-lg flex items-center justify-center mb-4">
            <svg class="w-5 h-5 text-amber-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6V4m0 2a2 2 0 100 4m0-4a2 2 0 110 4m-6 8a2 2 0 100-4m0 4a2 2 0 110-4m0 4v2m0-6V4m6 6v10m6-2a2 2 0 100-4m0 4a2 2 0 110-4m0 4v2m0-6V4"></path>
            </svg>
          </div>
          <h3 class="text-lg font-semibold text-white mb-2">Customizable Alerts</h3>
          <p class="text-gray-400 text-sm">Configure which errors trigger notifications and how often.</p>
        </div>
        
        <div class="bg-white/5 border border-white/10 rounded-xl p-6 hover:border-violet-500/30 transition">
          <div class="w-10 h-10 bg-pink-500/20 rounded-lg flex items-center justify-center mb-4">
            <svg class="w-5 h-5 text-pink-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.101m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656-5.656l-1.1 1.1"></path>
            </svg>
          </div>
          <h3 class="text-lg font-semibold text-white mb-2">Direct Links</h3>
          <p class="text-gray-400 text-sm">One-click access from Slack to the full error details in ActiveRabbit.</p>
        </div>
        
        <div class="bg-white/5 border border-white/10 rounded-xl p-6 hover:border-violet-500/30 transition">
          <div class="w-10 h-10 bg-red-500/20 rounded-lg flex items-center justify-center mb-4">
            <svg class="w-5 h-5 text-red-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 9v2m0 4h.01m-6.938 4h13.856c1.54 0 2.502-1.667 1.732-3L13.732 4c-.77-1.333-2.694-1.333-3.464 0L3.34 16c-.77 1.333.192 3 1.732 3z"></path>
            </svg>
          </div>
          <h3 class="text-lg font-semibold text-white mb-2">Spike Detection</h3>
          <p class="text-gray-400 text-sm">Get alerted when error rates spike above normal thresholds.</p>
        </div>
      </div>
    </div>
  </section>

  <!-- AI Disclaimer -->
  <section class="py-12 px-4 sm:px-6 lg:px-8">
    <div class="max-w-3xl mx-auto">
      <div class="bg-amber-500/10 border border-amber-500/20 rounded-2xl p-6">
        <div class="flex items-start space-x-4">
          <div class="flex-shrink-0 w-10 h-10 bg-amber-500/20 rounded-lg flex items-center justify-center">
            <svg class="w-5 h-5 text-amber-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 16h-1v-4h-1m1-4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path>
            </svg>
          </div>
          <div>
            <h3 class="text-lg font-semibold text-amber-200 mb-2">AI-Generated Content Disclaimer</h3>
            <p class="text-amber-200/70 text-sm leading-relaxed">
              ActiveRabbit uses AI (Large Language Models) to generate error summaries and fix suggestions. 
              While we strive for accuracy, AI-generated content may occasionally be inaccurate or incomplete. 
              Always review and verify AI suggestions before implementing them in your codebase.
            </p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Installation Guide -->
  <section class="py-20 px-4 sm:px-6 lg:px-8">
    <div class="max-w-3xl mx-auto">
      <div class="text-center mb-12">
        <h2 class="text-3xl sm:text-4xl font-bold text-white mb-4">Installation Guide</h2>
        <p class="text-gray-400">Get up and running in minutes.</p>
      </div>
      
      <div class="bg-white/5 border border-white/10 rounded-2xl p-8">
        <ol class="space-y-6">
          <li class="flex items-start">
            <span class="flex-shrink-0 w-8 h-8 bg-gradient-to-r from-violet-600 to-indigo-600 rounded-full flex items-center justify-center text-white font-bold text-sm mr-4">1</span>
            <div class="pt-1">
              <strong class="text-white">Sign up or log in</strong>
              <span class="text-gray-400"> to your ActiveRabbit account.</span>
            </div>
          </li>
          <li class="flex items-start">
            <span class="flex-shrink-0 w-8 h-8 bg-gradient-to-r from-violet-600 to-indigo-600 rounded-full flex items-center justify-center text-white font-bold text-sm mr-4">2</span>
            <div class="pt-1">
              <strong class="text-white">Create a project</strong>
              <span class="text-gray-400"> or select an existing one from your dashboard.</span>
            </div>
          </li>
          <li class="flex items-start">
            <span class="flex-shrink-0 w-8 h-8 bg-gradient-to-r from-violet-600 to-indigo-600 rounded-full flex items-center justify-center text-white font-bold text-sm mr-4">3</span>
            <div class="pt-1">
              <strong class="text-white">Go to Project Settings</strong>
              <span class="text-gray-400"> and find the "Slack Integration" section.</span>
            </div>
          </li>
          <li class="flex items-start">
            <span class="flex-shrink-0 w-8 h-8 bg-gradient-to-r from-violet-600 to-indigo-600 rounded-full flex items-center justify-center text-white font-bold text-sm mr-4">4</span>
            <div class="pt-1">
              <strong class="text-white">Click "Add to Slack"</strong>
              <span class="text-gray-400"> and authorize the app in your workspace.</span>
            </div>
          </li>
          <li class="flex items-start">
            <span class="flex-shrink-0 w-8 h-8 bg-gradient-to-r from-violet-600 to-indigo-600 rounded-full flex items-center justify-center text-white font-bold text-sm mr-4">5</span>
            <div class="pt-1">
              <strong class="text-white">Configure notification preferences</strong>
              <span class="text-gray-400"> — choose which alerts to receive and how often.</span>
            </div>
          </li>
          <li class="flex items-start">
            <span class="flex-shrink-0 w-8 h-8 bg-gradient-to-r from-violet-600 to-indigo-600 rounded-full flex items-center justify-center text-white font-bold text-sm mr-4">6</span>
            <div class="pt-1">
              <strong class="text-white">Test the integration</strong>
              <span class="text-gray-400"> by clicking "Test Slack Notification" button.</span>
            </div>
          </li>
        </ol>
      </div>
    </div>
  </section>

  <!-- Permissions -->
  <section class="py-20 px-4 sm:px-6 lg:px-8 bg-gradient-to-b from-transparent via-violet-600/5 to-transparent">
    <div class="max-w-3xl mx-auto">
      <div class="text-center mb-12">
        <h2 class="text-3xl sm:text-4xl font-bold text-white mb-4">Required Permissions</h2>
        <p class="text-gray-400">We request only the permissions necessary to deliver error notifications.</p>
      </div>
      
      <div class="bg-white/5 border border-white/10 rounded-2xl p-8">
        <ul class="space-y-4">
          <li class="flex items-start space-x-4">
            <code class="flex-shrink-0 bg-violet-500/20 text-violet-300 px-3 py-1 rounded-lg text-sm font-mono">chat:write</code>
            <span class="text-gray-400 pt-0.5">Send messages to channels where the bot is a member</span>
          </li>
          <li class="flex items-start space-x-4">
            <code class="flex-shrink-0 bg-violet-500/20 text-violet-300 px-3 py-1 rounded-lg text-sm font-mono">chat:write.public</code>
            <span class="text-gray-400 pt-0.5">Send messages to public channels without joining</span>
          </li>
          <li class="flex items-start space-x-4">
            <code class="flex-shrink-0 bg-violet-500/20 text-violet-300 px-3 py-1 rounded-lg text-sm font-mono">channels:read</code>
            <span class="text-gray-400 pt-0.5">View basic information about public channels</span>
          </li>
          <li class="flex items-start space-x-4">
            <code class="flex-shrink-0 bg-violet-500/20 text-violet-300 px-3 py-1 rounded-lg text-sm font-mono">channels:manage</code>
            <span class="text-gray-400 pt-0.5">Create and manage the alert channel</span>
          </li>
        </ul>
        
        <div class="mt-6 pt-6 border-t border-white/10">
          <p class="text-gray-500 text-sm">
            We only use these permissions to deliver error notifications. We do not read your messages or access any other data in your workspace.
          </p>
        </div>
      </div>
    </div>
  </section>

  <!-- Support -->
  <section class="py-20 px-4 sm:px-6 lg:px-8">
    <div class="max-w-3xl mx-auto text-center">
      <h2 class="text-3xl font-bold text-white mb-4">Need Help?</h2>
      <p class="text-gray-400 mb-8">Our team is here to help you get the most out of ActiveRabbit.</p>
      
      <div class="bg-white/5 border border-white/10 rounded-2xl p-8 mb-8">
        <a href="mailto:support@activerabbit.app" class="inline-flex items-center px-6 py-3 bg-gradient-to-r from-violet-600 to-indigo-600 hover:from-violet-500 hover:to-indigo-500 text-white font-medium rounded-lg transition shadow-lg shadow-violet-500/25">
          <svg class="w-5 h-5 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 8l7.89 5.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"></path>
          </svg>
